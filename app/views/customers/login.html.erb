<h1> Log In </h1>

<%= form_tag "handle_login" do %>
    <%= label_tag "Enter Username" %>
    <%= text_field_tag :username %>
    <%= label_tag "Enter Password" %>
    <%= password_field_tag :password %>
    <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
    <%= submit_tag "Log In" %>
<% end %>